  "security": {
    "whitelist_pattern": "^https://github\\.com/DudeAdrian/.*$",
    "allowed_operations": [
      "read", "write", "commit", "branch", "merge", "deploy"
    ],
    "confirmation_required": [
      "delete", "force_push", "merge_to_main", "production_deploy"
    ],
    "rate_limits": {
      "commits_per_hour": 10,
      "file_writes_per_hour": 20,
      "deployments_per_hour": 5,
      "deletions_per_hour": 3
    }
  },
  "jarvis_config": {
    "voice_model": "base",
    "llm_model": "llama3.1:70b",
    "ollama_host": "http://localhost:11434",
    "hive_api_url": "http://localhost:3000",
    "command_timeout": 30,
    "confirmation_timeout": 30,
    "autonomous_mode": true,
    "watch_interval": 900
  },
  "voice_commands": {
    "wake_words": ["sofie", "sophie", "hum", "jarvis"],
    "admin_phrases": [
      "status of all repos",
      "status of {repo}",
      "build {feature} in {repo}",
      "create {item} in {repo}",
      "check hive",
      "hive status",
      "deploy {repo}",
      "treasury",
      "nectar balance",
      "grid credits"
    ]
  },
  "ledger_integration": {
    "anchor_actions": true,
    "transparency_mode": "full",
    "hash_algorithm": "keccak256",
    "action_types": [
      "code_commit",
      "voice_command",
      "system_config",
      "deployment"
    ]
